#include <bits/stdc++.h>
using namespace std;

int main(){
    
    int pages;  //no of pages to be read
    cin>>pages;
    
    int week[7];   // array of no of pages he can read in days of weeks
    for(int i=0; i<7; i++){
        cin>>week[i];
    }
    
    int day;  //last day when he completes reading the book
    
    while(pages>0){
        for(int i=0; i<7; i++){
            if(pages>0){
                pages=pages-week[i];
                day=i+1;
            }
        }
    }
    
    cout<<day;  
    
    return 0;
}
